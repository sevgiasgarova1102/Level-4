#include <iostream>
using namespace std;

int main() {
    int a = 0;
    int b = 1;
    int next;

    cout << "1000-den kicik Fibonacci ededler: ";

    while (a < 1000) {
        cout << a << " ";

        next = a + b; 
        a = b;        
        b = next;     
    }

    return 0;
}
